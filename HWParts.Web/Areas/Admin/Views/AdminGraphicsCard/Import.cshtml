@model HWParts.Core.Application.ViewModels.GraphicsCard.ImportGraphicsCardsViewModel
@{
    ViewData["Title"] = "Importar Placas de Vídeo";
}

@using (@Html.BeginForm(
    "Import",
    "AdminGraphicsCard",
    FormMethod.Post,
    new { enctype = "multipart/form-data" }))
{
    @Html.TextBoxFor(x => x.File, new { type = "file" })

    <button type="submit" class="btn btn-block btn-primary">
        Importar
    </button>
}